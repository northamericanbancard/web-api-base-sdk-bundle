{
  "name": "nab/web-api-base-sdk-bundle",
  "type": "symfony-bundle",
  "description": "North American Bancard Generic Base SDK Bundle that allows for both AWS IAM, JWT, and No Auth",
  "license": "MIT",
  "authors": [
    {
      "name": "James Hollenbeck",
      "email": "jhollenbeck@nabancard.com"
    }
  ],
  "support": {
    "email": "jhollenbeck@nabancard.com"
  },
  "repositories": [
    {
      "type": "vcs",
      "url": "https://github.com/northamericanbancard/web-api-base-sdk-php.git"
    }
  ],
  "require": {
    "php": ">=5.5",
    "guzzlehttp/guzzle": "*",
    "aws/aws-sdk-php": "^3.36",
    "nab/web-api-base-sdk-php": "~1.0.0",
    "symfony/framework-bundle": "^2.8"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.0",
    "squizlabs/php_codesniffer": "^3"
  },
  "config": {
    "platform": {
      "php": "5.5.9"
    }
  },
  "autoload": {
    "psr-4": { "NAB\\Bundle\\WebApiBaseSdk\\": "" }
  }
}
